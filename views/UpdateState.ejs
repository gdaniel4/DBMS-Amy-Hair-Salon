<html>
    <head>
        <%- include('../views/head'); %> <!--Include head file-->
    </head>
<body class = "container">
    <header>
        <%- include('../views/header'); %> <!--Include header file-->
    </header>
    <main>
        <div style="padding: 20px; border-style: solid;">
        <form action = "/State/add" method = "POST" class = "Add">
            <% State.forEach(function(States) { %>
        <br><br><br>
        <label for = "StateInitial" style="margin-left: 10%;"> State Initial: </label>
        <input type = "text" id = "StateInitial" name = "StateInitial" value = "<%= States.StateInitial %>">
        <label for = "StateName" style="margin-left: 10%;"> State Name: </label>
        <input type = "text" id = "StateName" name = "StateName" value = "<%=States.StateName %>">
        <br><br>
        <% }); %>
        <input type = "submit">
        </form>
        </div>
    </main>
</body>
</html>